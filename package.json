{
  "name": "nemo-constracts",
  "description": "NEMO smart contracts",
  "version": "0.1.0",
  "private": true,
  "author": "NEMO Inc",
  "license": "MIT",
  "scripts": {
    "testnet:env": "rm .env && ln -s .env.fuji .env",
    "mainnet:env": "rm .env && ln -s .env.avax .env",
    "clean": "rimraf cache artifacts dist",
    "compile": "sol-compiler",
    "extract": "node scripts/abi_extract.js",
    "update:abi": "yarn compile && yarn extract",
    "build": "hardhat compile",
    "deploy:fuji": "hardhat run scripts/deploy_sias.js --network fuji",
    "upgrade:fuji": "hardhat run scripts/upgrade_sias.js --network fuji"
  },
  "dependencies": {
    "@0x/abi-gen": "^5.7.0",
    "@0x/sol-compiler": "^4.7.7",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@walletconnect/web3-provider": "^1.6.2",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "dotenv": "^11.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.5",
    "hardhat": "^2.9.3",
    "hardhat-abi-exporter": "^2.7.2",
    "hardhat-deploy": "^0.9.24",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.7",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-typechain": "^0.3.5",
    "hardhat-upgrades": "^0.0.0",
    "hardhat-watcher": "^2.1.1",
    "ipfs-http-client": "^52.0.1",
    "solidity-coverage": "^0.7.17",
    "use-wallet": "^0.9.0",
    "walletlink": "^2.1.8"
  },
  "devDependencies": {
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0"
  }
}
